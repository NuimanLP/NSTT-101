{
  "kind": "collectionType",
  "collectionName": "tours",
  "info": {
    "singularName": "tour",
    "pluralName": "tours",
    "displayName": "Tour"
  },
  "options": {
    "draftAndPublish": true
  },
  "pluginOptions": {},
  "attributes": {
    "EventName": {
      "type": "string"
    },
    "EventDescription": {
      "type": "text"
    },
    "TourDate": {
      "type": "datetime"
    },
    "Price": {
      "type": "decimal"
    },
    "AvailableSeat": {
      "type": "integer"
    },
    "Image": {
      "allowedTypes": [
        "images"
      ],
      "type": "media",
      "multiple": true
    },
    "Categories": {
      "type": "string"
    },
    "bookings": {
      "type": "relation",
      "relation": "oneToMany",
      "target": "api::booking.booking",
      "mappedBy": "tour"
    }
  }
}
